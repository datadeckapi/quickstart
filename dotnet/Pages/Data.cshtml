@page
@model DataModel
@{
}

<h1>
    View data
</h1>

<form method="post">
    <div class="form-group">
        <label>Product</label>
        <select asp-for="Product" class="form-control">
            <option value="bill">Bill</option>
            <option value="identity">Identity</option>
            <option value="credit_risk">Credit risk</option>
            <option value="switch">Switch</option>
            <option value="accounting">Accounting</option>
            <option value="employment">Employment</option>
            <option value="freelancer">Freelancer</option>
            <option value="sustainability">Sustainability</option>
        </select>
    </div>
    <div class="form-group">
        <label>Access token</label>
        <input asp-for="AccessToken" class="form-control" />
    </div>
    <div class="form-group mt-3">
        <input type="submit" value="Submit" class="btn btn-primary" />
    </div>
</form>

@if (string.IsNullOrEmpty(Model.JsonResponse) == false)
{
    
}
<div class="mt-3">
    <textarea asp-for="JsonResponse" rows="10" class="form-control" style="width:100%;"></textarea>
</div>